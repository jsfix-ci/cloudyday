{
 "name": "cloudyday",
 "displayName": "Cloudy Day",
 "version": "0.2.1",
 "description": "All ideas no execution: A seeder file for quick setting up of a React web-application.",
 "main": "index.js",
 "engines": {
  "node": "6.9.4"
 },
 "author": "zephinzer <hello-at-joeir-dot-net>",
 "contributors": [
  {
   "name": "zephinzer",
   "email": "hello-at-joeir-dot-net"
  }
 ],
 "bin": {
  "cloudyday": "./.bin/setup.js"
 },
 "license": "GPL-3.0",
 "repository": {
  "type": "git",
  "url": "https://github.com/zephinzer/cloudyday"
 },
 "scripts": {
  "build": "NODE_ENV=production node .bin/build.js",
  "ci": "NODE_ENV=test node index.js",
  "db-setup-dev": "./.bin/db-setup-dev",
  "db-teardown-dev": "./.bin/db-teardown-dev",
  "dev": "NODE_ENV=development node index.js",
  "init": "node ./.bin/init.js",
  "migrate": "npm run sequelize db:migrate",
  "preinstall": "./.bin/pre-install",
  "postinstall": "./.bin/post-install",
  "seed": "npm run sequelize db:seed:all",
  "sequelize": "node_modules/.bin/sequelize --config './config/sequelize.config.js'",
  "start": "NODE_ENV=production node index.js",
  "test": "npm run test-back && npm run test-eslint && npm run build",
  "test-eslint": "NODE_ENV=test ./node_modules/.bin/eslint .",
  "test-back": "NODE_ENV=test ./node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -- ./**/*.test.js",
  "test-back-watch": "./node_modules/.bin/mocha --watch ./**/*.test.js",
  "unmigrate": "npm run sequelize db:migrate:undo",
  "unseed": "npm run sequelize db:seed:undo:all",
  "update": "./.bin/update"
 },
 "dependencies": {
  "babel-core": "^6.21.0",
  "babel-loader": "^6.2.10",
  "babel-preset-es2015": "^6.18.0",
  "babel-preset-react": "^6.16.0",
  "babel-preset-react-hmre": "^1.1.1",
  "body-parser": "^1.15.2",
  "case": "^1.4.2",
  "chai": "^3.5.0",
  "cookie-parser": "^1.4.3",
  "copy-dir": "^0.3.0",
  "cron": "^1.2.1",
  "css-loader": "^0.26.1",
  "ejs": "^2.5.5",
  "enzyme": "^2.7.0",
  "eslint": "^3.12.2",
  "eslint-plugin-react": "^6.8.0",
  "express": "^4.14.0",
  "istanbul": "^0.4.5",
  "jsonwebtoken": "^7.2.1",
  "karma": "^1.3.0",
  "material-ui": "^0.16.6",
  "mocha": "^3.2.0",
  "moment": "^2.17.1",
  "morgan": "^1.7.0",
  "mysql": "^2.12.0",
  "prompt": "^1.0.0",
  "react": "^15.4.1",
  "react-addons-css-transition-group": "^15.4.2",
  "react-addons-test-utils": "^15.4.2",
  "react-addons-transition-group": "^15.4.2",
  "react-dom": "^15.4.1",
  "react-redux": "^5.0.2",
  "react-router": "^3.0.0",
  "react-tap-event-plugin": "^2.0.1",
  "reactstrap": "^4.0.0-alpha.8",
  "redux": "^3.6.0",
  "redux-saga": "^0.14.2",
  "sequelize": "^3.28.0",
  "sequelize-cli": "^2.5.1",
  "style-loader": "^0.13.1",
  "validate-npm-package-name": "^2.2.2",
  "webpack": "^1.14.0",
  "webpack-dev-middleware": "^1.9.0",
  "webpack-hot-middleware": "^2.14.0",
  "winston": "^2.3.0"
 }
}
