{
  "name": "cloudyday",
	"displayName": "Cloudy Day",
  "version": "0.0.2",
  "description": "All ideas no execution: A seeder file for quick setting up of a React web-application.",
  "main": "index.js",
  "engines": {
    "node": "6.9.2"
  },
  "author": "Joseph Matthias Goh <hello-at-joeir-dot-net>",
	"contributers": [
		{
			"name": "Joseph Matthias Goh",
			"email": "hello-at-joeir-dot-net"
		}
	],
  "license": "GPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/zephinzer/cloudyday"
	},
  "scripts": {
    "build": "NODE_ENV=production node .bin/build.js",
    "dev": "NODE_ENV=development node index.js",
    "eslint": "node_modules/.bin/eslint .",
    "migrate": "npm run sequelize db:migrate",
    "preinstall": "./.bin/pre-install",
    "postinstall": "./.bin/post-install",
    "seed": "npm run sequelize db:seed:all",
    "sequelize": "node_modules/.bin/sequelize -- --config './config/sequelize.config.js'",
    "start": "NODE_ENV=production node index.js",
    "test": "node_modules/.bin/mocha ./test",
    "unmigrate": "npm run sequelize db:migrate:undo",
    "unseed": "npm run sequelize db:seed:undo:all",
    "update": "./.bin/update",
    "verify": "npm run test && npm run eslint"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "ejs": "^2.5.5",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.2.1",
    "karma": "^1.3.0",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "morgan": "^1.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "sequelize": "^3.28.0",
    "sequelize-cli": "^2.5.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
    "winston": "^2.3.0"
  }
}
