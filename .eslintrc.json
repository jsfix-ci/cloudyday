{
	"parserOptions": {
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": [
		"react"
	],
	"env": {
		"browser": true,
		"es6": true,
		"node": true,
		"mocha": true
	},
	"rules": {
		"eol-last": 2,
		"eqeqeq": 2,
		"no-array-constructor": 2,
		"no-eval": 2,
		"no-else-return": 2,
		"no-fallthrough": 2,
		"no-implied-eval": 2,
		"no-iterator": 2,
		"no-label-var": 2,
		"no-labels": 2,
		"no-loop-func": 2,
		"no-native-reassign": 2,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-shadow": 2,
		"no-spaced-func": 2,
		"no-undefined": 2,
		"no-unused-vars": 2,
		"no-use-before-define": 2,
		"no-useless-call": 2,
		"no-useless-escape": 2,
		"no-useless-rename": 2,
		"no-var": 2,
		"no-void": 2,
		"react/display-name": 1,
		"react/jsx-sort-props": 2,
		"react/jsx-uses-vars" : 1,
		"react/jsx-uses-react": 1,
		"semi-spacing": 1
	}
}